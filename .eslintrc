{
  "env": {
    "browser": true,
    "webextensions": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-else-return": 0,
    "no-param-reassign": 0
  },
  "globals": {
    "Paths": false,
    "Downloads": false,
    "Variables": false,
    "options": false,
    "currentTab": false,

    "createExtensionNotification": false,
    "matchRules": false,
    "parseRules": false,
    "makeSeparator": false,
    "makeShortcut": false,
    "SPECIAL_DIRS": false,
    "RULE_TYPES": false,
    "PATH_SEGMENT_TYPES": false,
    "SHORTCUT_EXTENSIONS": false,
    "SHORTCUT_TYPES": false,
    "MESSAGE_TYPES": false,
    "DOWNLOAD_TYPES": false,
    "MEDIA_TYPES": false
  }
}
