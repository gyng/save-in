{
  "env": {
    "browser": true,
    "webextensions": true
  },
  "extends": [
    "airbnb",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-else-return": 0
  },
  "globals": {
    "SPECIAL_DIRS": true,
    "MEDIA_TYPES": true,
    "SHORTCUT_TYPES": true,
    "SHORTCUT_EXTENSIONS": true,
    "DOWNLOAD_TYPES": true,
    "RULE_TYPES": true,
    "EXTENSION_REGEX": true,
    "replaceSpecialDirs": true,
    "truncateIfLongerThan": true,
    "downloadInto": true,
    "addNotifications": true,
    "requestedDownloadFlag": true,
    "makeObjectUrl": true,
    "makeShortcut": true,
    "currentTab": true,
    "createExtensionNotification": true,
    "suggestShortcutFilename": true,
    "getFilenameFromUrl": true,
    "matchRules": true,
    "parseRules": true,
    "options": true,
    "replaceFsBadChars": true,
    "sanitizePath": true,
    "sanitizeFilename": true,
    "replaceLeadingDots": true,
    "removeSpecialDirs": true,
    "lastDownload": true,
    "rewriteFilename": true,
    "getCaptureMatches": true
  }
}
