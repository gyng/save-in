{
  "name": "save-in",
  "version": "2.7.0",
  "license": "MIT",
  "scripts": {
    "build": "env -u WEB_EXT_API_KEY -u WEB_EXT_API_SECRET web-ext build --overwrite-dest -i test docs yarn.lock yarn-error.log",
    "build:firefox:submit": "web-ext sign",
    "d": "env -u WEB_EXT_API_KEY -u WEB_EXT_API_SECRET web-ext run --verbose",
    "d:noenv": "web-ext run --verbose",
    "d:noenv:stable-x64": "web-ext run --verbose --firefox=\"C:/Program Files/Mozilla Firefox/firefox.exe\" -s=\".\"",
    "lint": "env -u WEB_EXT_API_KEY -u WEB_EXT_API_SECRET web-ext lint && eslint src test",
    "lint:fix": "eslint --fix src test",
    "mockserve": "node test/mockserver",
    "prettier": "echo 'ðŸ’ˆ Listing files to format. Run yarn prettier:write to write.\n' && prettier --list-different src/**/*.js src/*.js test/**/*.js test/*.js",
    "prettier:write": "prettier --write src/**/*.js src/*.js test/**/*.js test/*.js",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jest": "^21.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^21.2.1",
    "jest-webextension-mock": "^2.1.0",
    "koa": "^2.4.1",
    "koa-logger": "^3.1.0",
    "koa-route": "^3.2.0",
    "koa-static": "^4.0.2",
    "prettier": "^1.7.4",
    "web-ext": "^2.7.0"
  },
  "jest": {
    "setupFiles": [
      "jest-webextension-mock"
    ],
    "modulePathIgnorePatterns": [
      "<rootDir>/web-ext-artifacts/"
    ]
  },
  "dependencies": {}
}
